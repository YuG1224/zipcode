{
  "name": "zipcode",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/YuG1224/zipcode.git",
  "author": "Yuji Yamaguchi <yuji.yamaguchi24@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:client && yarn build:server",
    "build:client": "webpack --mode production --config webpack.client.js",
    "build:server": "node src/build/app.js && netlify-lambda build src/server",
    "start": "concurrently 'yarn start:client' 'yarn start:server'",
    "start:client": "webpack-dev-server --mode development --hot --config webpack.client.js",
    "start:server": "node src/build/app.js && netlify-lambda serve src/server -c webpack.server.js",
    "eslint": "eslint --ext .js --ignore-path .gitignore . --fix",
    "lint": "run-s prettier eslint",
    "prettier": "prettier --write \"./**/*.{js,json,md}\""
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "concurrently": "^4.0.1",
    "dotenv": "^6.0.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-prettier": "^3.0.0",
    "fs-extra": "^7.0.0",
    "lint-staged": "^7.3.0",
    "lodash": "^4.17.11",
    "netlify-lambda": "^0.4.0",
    "prettier": "^1.14.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "csvtojson": "^2.0.8",
    "encoding-japanese": "^1.0.29",
    "request": "^2.88.0",
    "unzipper": "^0.9.3"
  }
}
